<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Disable sandbox for system file access -->
	<key>com.apple.security.app-sandbox</key>
	<false/>

	<!-- File access permissions -->
	<key>com.apple.security.files.user-selected.read-write</key>
	<true/>

	<!-- Ability to read launch agents/daemons -->
	<key>com.apple.security.temporary-exception.files.absolute-path.read-write</key>
	<array>
		<string>/Library/LaunchAgents/</string>
		<string>/Library/LaunchDaemons/</string>
		<string>/System/Library/LaunchAgents/</string>
		<string>/System/Library/LaunchDaemons/</string>
	</array>

	<!-- Allow scripting to run launchctl -->
	<key>com.apple.security.temporary-exception.apple-events</key>
	<true/>
</dict>
</plist>
